
<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body text-center">
      <h1><i class="fab fa-node-js fa-3x"></i></h1>
      <p>Create an account or login</p>
      <a href="/users/registerUser" class="btn btn-primary btn-block mb-2">Register</a>
      <a href="/users/loginUser" class="btn btn-secondary btn-block">Login</a>
    </div>
  </div>
</div>
  <label id = 'message-container'></label>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.3.1/socket.io.js"></script>
<script type="text/javascript">
  function displayMessage(message) {
      const messageContainer = document.getElementById('message-container');
      messageContainer.innerHTML = message;
    }
const socket = io('http://localhost:3000');
socket.on('connect', ()=>{
  displayMessage('Connected with the id  ' + socket.id);
})
</script>
